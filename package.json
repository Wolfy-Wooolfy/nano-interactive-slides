{
  "name": "nano-interactive-slides",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "src/addins/*",
    "packages/*"
  ],
  "scripts": {
    "certs": "office-addin-dev-certs install",
    "start": "webpack serve --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "dev-addin": "pnpm --dir src/addins/powerpoint dev",
    "start-addin": "pnpm --dir src/addins/powerpoint exec office-addin-debugging start .\\manifest.xml desktop --app powerpoint --no-debug --no-live-reload",
    "stop-addin": "pnpm --dir src/addins/powerpoint exec office-addin-debugging stop"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-loader": "^5.1.0",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-dev-settings": "^3.0.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
